<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>UniFi Video Controller</Name>
  <Overview>
    UniFi Video Controller
  </Overview>
  <Description>
    UniFi Video Controller
  </Description>
  <Registry>https://github.com/rednut/docker-unifi-video-controller</Registry>
  <GitHub>https://github.com/rednut/docker-unifi-video-controller</GitHub>
  <Repository>https://github.com/rednut/docker-unifi-video-controller</Repository>
  <Support>.</Support>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>7222</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>7443</HostPort>
        <ContainerPort>7443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>7080</HostPort>
        <ContainerPort>7080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>6666</HostPort>
        <ContainerPort>6666</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>7447</HostPort>
        <ContainerPort>7447</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8880</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>4443</HostPort>
        <ContainerPort>443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>

    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/UniFi-Video</HostDir>
      <ContainerDir>/var</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:7443]</WebUI>
  <Banner>https://res.cloudinary.com/airos/image/upload/announcement/favicon-152.png</Banner>
  <Icon>https://res.cloudinary.com/airos/image/upload/announcement/favicon-152.png</Icon>
</Container>
