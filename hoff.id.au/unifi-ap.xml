<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>UniFi WiFi AP Controller</Name>
  <Overview>
    UniFi WiFi AP Controller
  </Overview>
  <Description>
    UniFi Controller
  </Description>
  <Registry>https://registry.hub.docker.com/u/jasonrm/unifi/</Registry>
  <GitHub>.</GitHub>
  <Repository>jasonrm/unifi</Repository>
  <Support>.</Support>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8085</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>9443</HostPort>
        <ContainerPort>8443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>27117</HostPort>
        <ContainerPort>27117</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>2221</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>2221</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/UniFi</HostDir>
      <ContainerDir>/var</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8080]</WebUI>
  <Banner>https://res.cloudinary.com/airos/image/upload/announcement/favicon-152.png</Banner>
  <Icon>https://res.cloudinary.com/airos/image/upload/announcement/favicon-152.png</Icon>
</Container>
